{
  "name": "pcd-viewer",
  "displayName": "pcd-viewer",
  "description": "an extension to preview pcd file in vscode.",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.57.0"
  },
  "publisher": "azurity",
  "author": {
    "name": "azurity",
    "email": "784227504@qq.com"
  },
  "homepage": "https://github.com/azurity/vscode-pcd-viewer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/azurity/vscode-pcd-viewer.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:pcdViewer.pcdPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "pcdViewer.pcdPreview",
        "displayName": "pcd preview",
        "selector": [
          {
            "filenamePattern": "*.pcd"
          }
        ],
        "priority": "default"
      }
    ],
    "configuration": {
      "title": "PCD Viewer",
      "type": "object",
      "properties": {
        "pcdViewer.backgroundColor": {
          "type": "string",
          "default": "#000000",
          "description": "pcd viewer background color",
          "pattern": "^#([\\da-fA-F]{3}|[\\da-fA-F]{6})$"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "colorjs.io": "^0.5.2"
  }
}
